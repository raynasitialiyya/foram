{
  "name": "geologi",
  "version": "1.0.1",
  "productName": "Geologi app",
  "description": "umur bumi dan fosilisasi",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Geologi App\"",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/Electron\\ tutorial\\ app-darwin-x64/Electron\\ tutorial\\ app.app electron-tutorial-app --out=release-builds --overwrite --icon=assets/icons/mac/icon.icns",
    "create-installer-win": "node installers/windows/createinstaller.js",
    "dist": "build"
  },
  "author": "yufitau",
  "license": "ISC",
  "devDependencies": {
    "electron": "^5.0.7",
    "electron-builder": "^21.1.1",
    "electron-packager": "^14.0.2"
  },
  "dependencies": {
    "electron-reload": "^1.4.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^3.0.4"
  },
  "build": {
    "appId": "com.yufi.tau",
    "win": {
      "target": "nsis",
      "icon": "assets/img/home.png"
    }
  }
}
